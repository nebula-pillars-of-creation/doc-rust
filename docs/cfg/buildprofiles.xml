<?xml version="1.0" encoding="UTF-8" ?>

<buildprofiles xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <variables>
        <primary-color>blueberry</primary-color>
        <header-logo>https://www.rust-lang.org/static/images/rust-logo-blk.svg</header-logo>
        <product-web-url>https://www.rust-lang.org</product-web-url>
        <custom-favicons>https://www.rust-lang.org/static/images/rust-logo-blk.svg</custom-favicons>
        <download-title>GitHub Repo</download-title>
        <download-page>https://github.com/nebula-pillars-of-creation/doc-rust</download-page>
        <showDownloadButton>true</showDownloadButton>
        <web-root>https://some.domain</web-root>
    </variables>
    <build-profile instance="doc-rust"/>
    <footer>
        <copyright>Gishant Singh 2023</copyright>
    </footer>
</buildprofiles>
